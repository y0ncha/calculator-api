// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Operation {
  rawid     Int    @id @default(autoincrement())
  flavor    String
  operation String
  result    Int
  arguments String

  @@map("operations")
}
