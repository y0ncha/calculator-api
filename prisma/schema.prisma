generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Operation {
  rawid     Int     @id @map("rawid")
  flavor    String? @map("flavor") @db.VarChar(255)
  operation String? @map("operation") @db.VarChar(255)
  arguments String? @map("arguments") @db.VarChar(255)
  result    Int?    @map("result")

  @@map("operations")
}
